{
    "suggest.noselect": true,
    "suggest.preferCompleteThanJumpPlaceholder": true,
    "languageserver": {
        "reason": {
            "command": "~/.dotfiles/lib/reason-language-server/reason-language-server-darwin",
            "filetypes": ["reason"]
        },
        "terraform": {
            "command": "~/.dotfiles/lib/terraform-lsp/terraform-lsp-darwin",
            "filetypes": ["terraform"]
        }
    },
    "coc.preferences.formatOnType": true,
    "eslint.enable": false
}
