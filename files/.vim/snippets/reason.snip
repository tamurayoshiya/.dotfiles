snippet s
options word
    {j|${1}|j}

snippet rs
options word
    React.string

snippet /*
options word
    /* ${1} */

snippet /**
options word
    /**
     * ${1}
     */

snippet style
options word
    style={ReactDOMRe.Style.make(~fontSize="1rem", ())}

snippet switch
options word
    switch(${1}) {
    | Some(${2}) => ()
    | None => ()
    }
